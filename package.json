{
  "name": "blog",
  "version": "1.0.0",
  "description": "geraldchen's blog",
  "author": "geraldchen890806@gmail.com",
  "main": "server.js",
  "scripts": {
    "postinstall": "npm run build:dll",
    "build:dll": "webpack-cli --display-chunks --color --config ./config/webpack.config.dll.js  --hide-modules",
    "start": "export NODE_ENV=development && node server.js",
    "deploy": "export NODE_ENV=production && webpack --config ./config/webpack.config.production.js && npm start",
    "build": "rm -rf staticPro && NODE_ENV=production && webpack-cli -p --config ./config/webpack.config.production.js --progress",
    "forever": "rm -rf static && cp -r staticPro static && export NODE_ENV=production PORT=80 SSLPORT=443 && forever start server.js",
    "testSW": "export NODE_ENV=production PORT=3022 SSLPORT=3021 && `npm bin`/webpack --config ./config/webpack.config.production.js && npm start",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:staged": "lint-staged"
  },
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix"
      ]
    },
    "ignore": []
  },
  "pre-commit": "lint:staged",
  "dependencies": {
